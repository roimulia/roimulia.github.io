    <script src="plugins/jqueryForm/jquery.form.js"></script> 
 
    <script> 
        // wait for the DOM to be loaded 
        $(document).ready(function() { 
            // bind 'myForm' and provide a simple callback function 
			
			// מוליה כאן אני מחבר את הפורם לפלאגין
            $('#contactForm').ajaxForm(function(response) {
				if(response == "true"){
					alert("Messege sent"); 
				}else{
					if(response == "fale"){
						alert("Damn, we have a problem."); 
					}
				}
            }); 
        }); 
    </script> 
