	<script> 
	function move()
	{

	 $("html, body").animate({ scrollTop: $('#proto').offset().top }, 1000);

	} 

	function move2()
	{

	 $("html, body").animate({ scrollTop: $('#emails').offset().top }, 1000);

	} 
	</script>

